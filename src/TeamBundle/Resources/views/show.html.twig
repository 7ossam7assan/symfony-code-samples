    {% extends 'base.html.twig' %}

{% block body %}

    <div class="page-content">

        <div class="page-head">
            <!-- BEGIN PAGE TITLE -->
            <div class="page-title">
                <h1>الفرق </h1>
            </div>
            <!-- END PAGE TITLE -->
        </div>
        <!-- END PAGE HEAD-->
        <!-- BEGIN PAGE BREADCRUMB -->
        <ul class="page-breadcrumb breadcrumb">
            <li>
                <a href="#">الرئيسيه</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <a href="{{ path('team_index') }}">الفرق</a>
                <i class="fa fa-circle"></i>
            </li>
            <li>
                <span class="active">عرض</span>
            </li>
        </ul>
        <!-- END PAGE BREADCRUMB -->


        <div class="row">
            <div class="col-md-12">

                <div>
                    <div class="portlet box blue">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-plus"></i> عرض </div>
                            <div class="tools">
                            </div>
                        </div>
                        <div class="portlet-body form">
                            <!-- BEGIN FORM-->
                            <form action="" method="post" class="form-horizontal">
                                <div class="form-body">
                                    {#<h3 class="form-section">Person Info</h3>#}


                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3">العنوان  <span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <input type="text" data-required="1" value="{{ team.title }}" id="title" name="title" readonly class="form-control" placeholder="العنوان">
                                                    {#<span class="help-block"> This is inline help </span>#}


                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3">العنوان الوظيفى <span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <input type="text" data-required="1"  value="{{ team.jobTitle }}" id="job_title" readonly name="job_title" class="form-control" placeholder=" العنوان الوظيفى">
                                                    {#<span class="help-block"> This is inline help </span>#}


                                                </div>
                                            </div>
                                        </div>

                                    </div>


                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3"> المكان<span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <input type="text" data-required="1" value="{{ team.place }}" id="place" name="place" readonly class="form-control" placeholder="المكان">
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3"> البريد الالكترونى<span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <div class="input-group">
                                                    <span class="input-group-addon">
                                                        <i class="fa fa-envelope"></i>
                                                    </span>
                                                        <input type="text" value="{{ team.email }}" name="email" class="form-control" readonly placeholder=" البريد الالكترونى">
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3"> رقم الهاتف<span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <input type="text" data-required="1" value="{{ team.phone }}"  id="phone" name="phone" readonly class="form-control" placeholder="رقم الهاتف">
                                                </div>
                                            </div>
                                        </div>

                                    </div>



                                    <div class="row">
                                        <div class="col-md-8">
                                            <div class="form-group">
                                                <label class="control-label col-md-3">Slug <span class="required"> * </span></label>

                                                <div class="col-md-9">
                                                    <input type="text" data-required="1" value="{{ team.slug }}" id="slug" name="slug" class="form-control" placeholder="Slug" readonly>
                                                    {#<span class="help-block"> This is inline help </span>#}

                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label class="control-label col-md-2">الوصف</label>
                                            <div class="col-md-9">
                                                 <textarea class="wysihtml5 form-control" style="text-align: right;"  rows="6" placeholder="الوصف" name="description" disabled>
                                                     {{ team.description }}
                                                 </textarea>
                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <h3 class="form-section">روابط شبكات التواصل الاجتماعى</h3>

                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">فيسبوك </label>

                                            <div class="col-md-9">
                                                <input type="text" data-required="1" readonly value="{{ team.fbLink }}" id="fb-link" name="fb-link" class="form-control" placeholder="رابط الفيس بوك">
                                                {#<span class="help-block"> This is inline help </span>#}

                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">تويتر </label>

                                            <div class="col-md-9">
                                                <input type="text" data-required="1" readonly value="{{ team.twitterLink }}"  id="twitter_link" name="twitter-link" class="form-control" placeholder="رابط تويتر">
                                                {#<span class="help-block"> This is inline help </span>#}

                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">انستجرام </label>

                                            <div class="col-md-9">
                                                <input type="text" data-required="1" readonly value="{{ team.instagramLink }}"  id="instagram_link" name="instagram-link" class="form-control" placeholder="رابط انستجرام">
                                                {#<span class="help-block"> This is inline help </span>#}

                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">يوتيوب </label>

                                            <div class="col-md-9">
                                                <input type="text" data-required="1" readonly value="{{ team.youtubeLink }}" id="youtube_link" name="youtube-link" class="form-control" placeholder="رابط يوتيوب">
                                                {#<span class="help-block"> This is inline help </span>#}

                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">سكايب </label>

                                            <div class="col-md-9">
                                                <input type="text" data-required="1" readonly value="{{ team.skypeLink }}" id="skype_link" name="skype-link" class="form-control" placeholder="رابط سكايب">
                                                {#<span class="help-block"> This is inline help </span>#}

                                            </div>
                                        </div>
                                    </div>

                                </div>



                                <h3 class="form-section">SEO</h3>
                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Meta Title</label>
                                            <div class="col-md-9">
                                                <input type="text"  onkeyup="countChar(this,'metaTitle')" data-required="1"  id="meta_title" name="meta_title" value="{{ team.metaTitle }}" class="form-control" placeholder="Meta Title" disabled>
                                                {#<span class="help-block"> This is inline help </span>#}
                                                <span class="help-block" id="metaTitle">  </span>


                                            </div>
                                        </div>
                                    </div>

                                </div>



                                <div class="row">
                                    <div class="col-md-8">
                                        <div class="form-group">
                                            <label class="control-label col-md-3">Meta Keywords</label>
                                            <div class="col-md-9">
                                                {#<input type="text" data-required="1"  id="meta_keywords" name="meta_keywords" value="{{ project.metaKeywords }}" class="form-control" placeholder="Meta Keywords">#}
                                                <input type="text" class="form-control" name="meta_keywords" value="{{ team.metaKeywords }}" data-role="tagsinput" disabled>

                                                {#<span class="help-block"> This is inline help </span>#}


                                            </div>
                                        </div>
                                    </div>

                                </div>

                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="form-group">
                                            <label class="control-label col-md-2">Meta Description</label>
                                            <div class="col-md-9">
                                                 <textarea class="wysihtml5 form-control" style="text-align: right;"  rows="6" placeholder="Meta Description" name="meta_description" disabled>
                                                 {{ team.metaDescription }}
                                                 </textarea>
                                            </div>
                                        </div>
                                    </div>

                                </div>


                                <div class="thumbnail">
                                    <img src="{{ asset('uploads/Teams/'~team.photo~'') | imagine_filter('team_thumb') }}">
                                </div>


                                {#<h3 class="form-section">Person Info</h3>#}



                        </form>
                        <!-- END FORM-->
                    </div>
                </div>
            </div>

        </div>
    </div>



    </div>




{% endblock %}

